{% extends "../_layout.twig" %}
{% set pageTitle = 'Plenco Wireframes' %}
{% set pageClass = 'body_plenco_wireframes' %}

{% set page_plenco_wireframes = {
    heading: {
        heading_top_text: "Research & Development",
        bg_img: "./assets/img/pages/plenco-wireframes/header.jpg",
        class: "body_research-development"
    },
    nav_side: {
        visible: false,
    }
}%}



{% block header %}
    {% include '../layouts/_header.twig' %}
{% endblock %}



{% block content %}
    {% include '../layouts/nav/_nav_side.twig'  with {data: page_plenco_wireframes.nav_side} %}

    <div class="section_43 p-0 m-0 overflow-hidden position-absolute top-0 left-0  h-100 w-100">
        <div class="header-image-cont w-100 h-50 overflow-hidden position-relative">
            <img src="./assets/img/pages/plenco-wireframes/header.jpg" alt="" class="header-image w-100">
        </div>
        <div class="content position-relative d-flex align-items-center flex-column justify-content-between">
            <img src="./assets/img/svg/logo_plenco_white.svg" alt="Plenco logo" class="plenco-logo position-relative">
            <p class="text_heading_main text-white-1 section_43__header mb-0 d-none d-md-block mt-4"><span class="text-blue-l2">Desktop</span> website wireframe design</p>
            <p class="text_heading_main text-white-1 section_43__header mb-0 d-md-none text-center"><span class="text-blue-l2">Mobile</span> website<br>wireframe design</p>
            <a href="preloader.html" class="section_43__button btn btn-yellow-1 px-5 w-auto d-none d-md-flex">Desktop Wireframes</a>
            <a href="preloader.html" class="section_43__button btn btn-yellow-1 px-5 w-auto d-md-none">Mobile Wireframes</a>
            <p class="section_43__version tag-text text-white-1 text-center mb-0 mt-sm-5 mt-md-0">Version 1.0<br>Proof: 11/05/21</p>
            <img src="./assets/img/svg/logo_5-color.svg" alt="Logo" class="footer__logo mb-5">

        </div>
    </div>



{% endblock %}


{% block footer %}
{% endblock %}