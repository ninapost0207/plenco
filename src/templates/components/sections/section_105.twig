<div class="row section_105_bg overflow-hidden w-100 {{ data.class }}" style="{{ data.style }}" id="section_16">
    <div class="col-12 px-0">
        <div class="container cont-site section_105">
            <div class="section_105__header_wrap mb-0 px-0">
                <h3 class="section_105__header text_bicolor mb-0 pb-0 text-start">{{ data.header }}</h3>
            </div>
            
            <p class="section_105__text text-start pt-4 mb-0">{{ data.text }}</p>
            <p class="section_105__text_add mb-0 text-start pt-4">{{ data.text_add }}</p>

            <div class="row row-cols-1 row-cols-md-2 section_105__cards mt-4 mx-auto gx-5 gy-5">
                {% for card in data.cards %}
                <a href={{ card.link_to }} class="section_105__link text-decoration-none">
                    <div class="section_105__card_item-wrap h-100">
                        <div class="card section_105__card_item col-12 border-0">
                            <img src={{ card.img }} class="section_105__card-img-top col-12" alt={{ card.alt }}>
                            <div class="section_105__card-body bg-blue-1 p-0 d-flex align-items-center col-12 position-relative">
                                <div class="section_105__card__line bg-yellow-1 mx-4 position-relative" style="height: 107px; width: 7px"></div>
                                <h5 class="section_105__card-title mb-0">{{ card.text }}</h5>                        
                            </div>
                        </div>
                    </div>
                </a>
                {% endfor %}
            </div>
        
        </div>
    </div>
</div>